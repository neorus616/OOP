#include <stdio.h>
char *append(const char *oldstring, const char *c){    int result;    char *newstring;    result = asprintf(&newstring, "%s%s", oldstring, c);    if (result == -1) newstring = NULL;    return newstring;}
int main(int argc, char *argv[]){    char *s = "stringed";    char *s2;    char *s3 = "";    char *s4 = "stringed";    char *s5;    //s5 = concat(s4,s5);    char *c = "XX";    s2 = append(s, c);    s5 = append(s3,s4);    if (s2 == NULL) { printf("allocation failed\n"); return 1;    }    //printf("appending %c to %s gives %s\n", c, s, s2);    //free(s2);    s = s2;    printf("%s\n",s5);    return 0;}

